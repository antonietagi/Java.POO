public class Main {
 public static void main (String [] args){
  Produto p1 = new Produto (1232, "Celular Samsung", 30);
  
  Produto p2 = new Produto ();
  p2.setCodigo (3456);
  p2.setDescricao("Monitor Samsung SyncMaster");
  p2.setQuantidadeArmazenada (12);
  
  p1.imprimeProduto();
  p1.imprimeTotalProduto();
  p1.statusEstoque();
  
  p2.imprimeProduto();
  p2.imprimeTotalProduto();
  p2.statusEstoque();
  
  p1.RetiraEstoque(5);
  p2.RetiraEstoque(8);
  
  System.out.println("DADOS DEPOIS DA RETIRADA DE ESTOQUE:");
  p1.imprimeProduto();
  p1.imprimeTotalProduto();
  p1.statusEstoque();
  
  p2.imprimeProduto();
  p2.imprimeTotalProduto();
  p2.statusEstoque();
  
  //public void entradaValorLimite () {
   
  //}
  
 }
}
